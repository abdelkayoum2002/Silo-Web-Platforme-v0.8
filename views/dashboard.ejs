                    }
                    if (index === 2) {
                        td.textContent = getTime(formatDateTime(time));
                        td.title = formatDateTime(time);
                    }
                    tr.appendChild(td);
                    });

                    tbody.appendChild(tr);
                } else {
                    return;
                }
            }
        }
    }
      
